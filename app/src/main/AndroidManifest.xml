<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.media_security">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Media Security"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Logout"></activity>
        <activity android:name=".Information" />
        <activity android:name=".VideoGallery" />
        <activity android:name=".VideoOption" />
        <activity android:name=".FilesList" />
        <activity android:name=".FilesOption" />
        <activity android:name=".ShowImage" />
        <activity android:name=".ImagesGallery" />
        <activity android:name=".ImagesOption" />
        <activity android:name=".ContactsOption" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Sign_up" />
        <activity
            android:name=".start"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>